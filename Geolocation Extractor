from geopy.geocoders import Nominatim
def get_geolocation(place):
    g = geocoder.arcgis(str(place))
    return g.latlng
def map_geo_location(place):
    return geo_coordinates[place]
    
geo_coordinates = {}  ## global variable, calculate only once
exclude_places = []
for place in data['Loc'].unique():
    try:
        geo_coordinates[place] = get_geolocation(place)
    except Exception as error:
        exclude_places.append(place)
        print(place)
        print(error)
for exclude_place in exclude_places:
    data = data[data['Loc'] != exclude_place]
